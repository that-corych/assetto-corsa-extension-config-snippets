        "prefix": "material_carpaint_chrome",
        "description": "Includes carpaint chrome materials base",
        "body": [
            "[Material_CarPaint_Chrome]",
            "CarPaintMaterial = Material",
            "FresnelMax = 1",
            "FresnelC = 0.4",
