    "Include carpaint metallic materials library":{
        "prefix": "material_carpaint_metallic",
        "description": "Includes carpaint metallic materials base",
        "body": [
            "[Material_CarPaint_Metallic]",
            "CarPaintMaterial = Material",
            "FresnelMax = 1",
            "FresnelC = 0.1",
            "BrightnessAdjustment = 0.9",
            "ColoredSpecular = 0.9",
            "AmbientSpecular = 0.6",
            "AmbientSpecularEXP = 2.5",
            "ClearCoatThickness = 0.06"
        ]
    },

